{
  "name": "grocery",
  "version": "0.1.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "husky install",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "format": "prettier --print-width 150 --write \"src/**/*.ts\" --loglevel error",
    "start": "node dist/src/main.js",
    "start:dev": "cross-env NODE_ENV=dev PORT=4000 nodemon dist/src/main.js",
    "start:debug": "cross-env NODE_ENV=dev DEBUG=true PORT=4000 nodemon dist/src/main.js",
    "build:dev": "tsc -p tsconfig.json -w",
    "lint": "./node_modules/.bin/tslint -c tslint.json -p tsconfig.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@nestjs/common": "9.3.2",
    "@nestjs/core": "9.3.2",
    "@nestjs/microservices": "9.3.2",
    "@nestjs/platform-express": "9.3.2",
    "@nestjs/swagger": "^6.1.0",
    "@nestjs/typeorm": "9.0.0",
    "assert": "^2.0.0",
    "bcrypt": "^5.0.1",
    "cache-manager": "^5.1.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "husky": "^8.0.1",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "pg": "^8.8.0",
    "pino": "^8.4.2",
    "pino-pretty": "^9.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.9"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.4",
    "@nestjs/testing": "9.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --print-width 150 --write",
      "git add"
    ]
